#include<bits/stdc++.h>
using namespace std;

int main (){
    int t ;
    cin>> t;
    while (t--)
    {
        int n , k;
        cin>>n>>k;
        
        string ans;
        
    for(int j = 0 ; j <n ; j++){
        char a = 'a';
    for(int i =0  ; i < k ; i ++){
        ans.push_back(a);
        a++;
    }
    }
    
    
        
    
    cout<<ans<<endl;

    }
    
}
